    "pg": "^8.12.0",
    "pinia": "^2.2.1",
    "pino": "^9.4.0",
    "pino-loki": "^2.3.0",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2",
    "toml": "^3.0.0",
    "uuid": "^9.0.1",
    "vue": "^3.4.23",
    "vue-class-component": "^7.2.6",
    "vue-dd": "^1.6.5",
    "vue-router": "^4.3.2",
    "vue3-json-viewer": "^2.2.2",
    "vuex": "^4.1.0"
  }
}
