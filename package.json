{
  "name": "altaira",
  "version": "0.0.1",
  "description": "DeathMatch server",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev:web:serve": "cd ./src/web && vite serve --host",
    "dev:db:codegen": "npx tsx kysely/index/codegen.ts",
    "dev:migrate:create": "npx tsx kysely/index/make-migration.script.ts",
    "dev:migrate:up": "npx tsx kysely/index/migrate.ts all && npm run db:codegen",
    "dev:migrate:next": "npx tsx kysely/index/migrate.ts up && npm run db:codegen",
    "dev:migrate:down": "npx tsx kysely/index/migrate.ts down && npm run db:codegen",
    "dev:up": "node scripts/cli.mjs up",
    "dev:down": "node scripts/cli.mjs down",
    "serve": "node build-scripts/index.js",

    "dev:docker": "docker-compose -f ./docker-compose.dev.yml up --build",
    "monitoring": "docker-compose -f ./docker-compose.monitoring.yml up -d",

    "build-docker": "docker-compose -f docker-compose.yml up --build",
    "build": "node build-scripts/prod.js",
    "migrate": "npx tsx kysely/index/migrate.ts all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@altv/types-client": "^16.2.3",
    "@altv/types-natives": "^16.2.0",
    "@altv/types-server": "^16.2.2",
    "@altv/types-shared": "^16.2.1",
    "@altv/types-webview": "^16.2.1",
    "@esbuild-plugins/node-resolve": "^0.2.2",
    "@eslint/js": "^9.8.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@stylistic/eslint-plugin-js": "^2.6.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.2.0",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vue/cli-service": "^5.0.8",
    "altv-esbuild": "^0.8.3",
    "altv-pkg": "^2.7.5",
    "chalk": "^5.3.0",
    "chalk-animation": "^2.0.3",
    "chokidar": "^4.0.0",
    "esbuild": "^0.23.0",
    "esbuild-node-externals": "^1.14.0",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.9.0",
    "js-yaml": "^4.1.0",
    "kysely-codegen": "^0.15.0",
    "leaflet": "^1.9.4",
    "npx": "^10.2.2",
    "prettier": "3.2.5",
    "rollup": "^4.20.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-swc3": "^0.11.2",
    "sass": "^1.75.0",
    "shalk": "0.0.1-security",
    "tailwindcss": "^3.4.3",
    "terminal-kit": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-to-swcconfig": "^2.7.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
